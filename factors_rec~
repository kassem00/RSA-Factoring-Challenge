#!/usr/bin/python3

import sys

def fun(x, i):
        if (x % i)  == 0 and i <= x:
            return (i)
        else:
            i += 1
            fun(x, i)

file_ = open(sys.argv[1], "r")
li = file_.readlines()

for x in range(len(li)):
    temp = int(li[x])
    i = 2
    i = fun(temp, i)
    z = temp // i
    print("{}={}*{}".format(temp, z, i))
    break