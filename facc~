import sys
import math


def factorize(num):
    factors = []
    i = 2
    while i * i <= num:
        if num % i:
            i += 1
        else:
            num //= i
            factors.append(i)
    if num > 1:
        factors.append(num)
    return factors


file_path = sys.argv[1]

with open(sys.argv[1], "r") as file_:
    li = file_.readlines()

for line in li:
    temp = int(line.strip())
    factors = factorize(temp)
    factor_str = "*".join(str(f) for f in factors)
    print("{}={}".format(temp, factor_str))

